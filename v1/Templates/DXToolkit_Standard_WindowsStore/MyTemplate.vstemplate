      <ProjectItem ReplaceParameters="false" TargetFileName="ScreenBase.h">TOOLKITHELPER\ScreenBase.h</ProjectItem>
      <ProjectItem ReplaceParameters="false" TargetFileName="ScreenManager.h">TOOLKITHELPER\ScreenManager.h</ProjectItem>
      <ProjectItem ReplaceParameters="false" TargetFileName="pch.h">pch.h</ProjectItem>
      <ProjectItem ReplaceParameters="false" TargetFileName="SpriteFontRenderer.h">CONTENT\SpriteFontRenderer.h</ProjectItem>
      <ProjectItem ReplaceParameters="false" TargetFileName="SimpleRenderer.h">CONTENT\SimpleRenderer.h</ProjectItem>
