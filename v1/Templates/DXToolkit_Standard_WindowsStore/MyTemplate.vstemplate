      <ProjectItem ReplaceParameters="false" TargetFileName="SmallLogo.png">ASSETS\SmallLogo.png</ProjectItem>
      <ProjectItem ReplaceParameters="false" TargetFileName="star.png">ASSETS\star.png</ProjectItem>
      <ProjectItem ReplaceParameters="false" TargetFileName="star2.png">ASSETS\star2.png</ProjectItem>
      <ProjectItem ReplaceParameters="false" TargetFileName="StoreLogo.png">ASSETS\StoreLogo.png</ProjectItem>
